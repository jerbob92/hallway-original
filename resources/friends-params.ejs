{
  "name": "limit",
  "description": "the number of results to return (20 if unspecified)",
  "paramType": "query",
  "required": false,
  "defaultValue": "2",
  "allowMultiple": false,
  "dataType": "string"
},
{
  "name": "offset",
  "description": "where to start in the list of friends",
  "paramType": "query",
  "required": false,
  "allowMultiple": false,
  "dataType": "string"
},
{
  "name": "sort",
  "description": "how to sort the list, default by firstname (see docs)",
  "paramType": "query",
  "required": false,
  "allowMultiple": false,
  "dataType": "string",
  "allowableValues":{
    "valueType":"LIST",
    "values":["first", "last", "connected", "interactions", "activity", "photos"]
  }
},
{
  "name": "q",
  "description": "Will return results with name or handle beginning with this string",
  "paramType": "query",
  "required": false,
  "allowMultiple": false,
  "dataType": "string"
},
{
  "name": "bio",
  "description": "Will return results that contain this string in the profile fields",
  "paramType": "query",
  "required": false,
  "allowMultiple": false,
  "dataType": "string"
},
{
  "name": "toc",
  "description": "Return a table of contents, giving offsets for each first letter",
  "paramType": "query",
  "required": false,
  "allowMultiple": false,
  "dataType": "boolean",
  "allowableValues":{
    "valueType":"LIST",
    "values":["false", "true"]
  }
},
{
  "name": "services",
  "description": "A list (comma seperated) of services to return results from",
  "paramType": "query",
  "required": false,
  "allowMultiple": true,
  "dataType": "string"
},
{
  "name": "full",
  "description": "return the full original profile data from the services (very verbose)",
  "paramType": "query",
  "required": false,
  "allowMultiple": false,
  "dataType": "boolean",
  "allowableValues":{
    "valueType":"LIST",
    "values":["false", "true"]
  }
}