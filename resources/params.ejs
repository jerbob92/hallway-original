{
  "name": "limit",
  "description": "the number of results to return (20 if unspecified, unlimited max)",
  "paramType": "query",
  "required": false,
  "defaultValue": "2",
  "allowMultiple": false,
  "dataType": "string"
},
{
  "name": "since",
  "description": "The unix timestamp (in epoch milliseconds) of the oldest entry. Defaults to 0, useful when looking for newer things since the last request",
  "paramType": "query",
  "required": false,
  "allowMultiple": false,
  "dataType": "string"
},
{
  "name": "until",
  "description": "The unix timestamp (in epoch milliseconds) of the newest entry. Defaults to now. Use this to page back in time, passing in the oldest timestamp of the previous results",
  "paramType": "query",
  "required": false,
  "allowMultiple": false,
  "dataType": "string"
},
{
  "name": "q",
  "description": "Will return results that match a full text search on text, name, nickname, etc",
  "paramType": "query",
  "required": false,
  "allowMultiple": false,
  "dataType": "string"
},
{
  "name": "participants",
  "description": "filters the result set to include the given comma-delimited user ids as all having participated",
  "paramType": "query",
  "required": false,
  "allowMultiple": true,
  "dataType": "string"
},
{
  "name": "near",
  "description": "A lat/lon (comma seperated) for geo/location filtering",
  "paramType": "query",
  "required": false,
  "allowMultiple": true,
  "dataType": "string"
},
{
  "name": "within",
  "description": "A distance in km for geo/location filtering",
  "paramType": "query",
  "required": false,
  "allowMultiple": false,
  "dataType": "string"
},
{
  "name": "map",
  "description": "Adds an extra field to each item in the JSON response containing normalized data at item.map.name",
  "paramType": "query",
  "required": false,
  "allowMultiple": false,
  "dataType": "boolean",
  "allowableValues":{
    "valueType":"LIST",
    "values":["true", "false"]
  }
}